 <script>
  var content_manila    = "http://localhost/FilPortTrackingSystem/Pagination/pagination_manila";
  var total_manila_page     =  "9";

 var content_manila_search    = "http://localhost/FilPortTrackingSystem/Pagination/search_paging";
  
