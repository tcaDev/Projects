		           method: "POST",
	 		       url: "http://localhost/FilPortTrackingSystem/Job_air_update/jobfile_add_charge_air",
	 		       beforeSend: function() {
					 	  dia_running_charges =	$.dialog({
					 	  	    icon: 'fa fa-spinner fa-spin',
					 	  	    closeIcon: false,
				        		title: 'Please wait!',
				        		backgroundDismiss: false,
				        		content: 'Currently Updating Running Charges',
				   			});
 					  },
			  	   data: {
			  	   	           jbfl   		:jbfl,
			  	   			   lodge        :lodge,
			  	   			   break_airs   :break_airs,
			  	   			   bad_cargo    :bad_cargo,
			  	   			   storage      :storage,
			  	   			   vrc          :vrc,
			  	   			   cni          :cni,
			  	   			   cniu         :cniu,
			  	   			   otherfee 	:otherfee
/*			                   cont_deposit :cont_deposit,   
			                   thc_charges  :thc_charges,
			                   wharfage     :wharfage,
			                   arrastre     :arrastre,
			                   weight	    :weight,
