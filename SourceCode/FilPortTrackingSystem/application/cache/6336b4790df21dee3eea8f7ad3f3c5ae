   });


  $('.view_goods-air').click(function(){
 		var jobfileno =  $(this).closest('tr').children('td:eq(2)').text();
 		   
  				$.ajax({
                    method: "POST",
                      url: "http://localhost/FilPortTrackingSystem/Job/get_goods_air",
                    data: { id:jobfileno,
                    		
                    }
                })
                .done(function(data) {
                	
                    $('.list_products_air').html(data);
                });
        });


