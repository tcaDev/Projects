     var lodge        = $('#runchar-air .lodge_airs').val().replace(/,/g,'');
     var break_airs   = $('#runchar-air .break_airs').val().replace(/,/g,'');
     var bad_cargo    = $('#runchar-air .bad-cargo_airs').val().replace(/,/g,'');
